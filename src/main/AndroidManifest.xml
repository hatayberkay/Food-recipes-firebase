        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="something.about.hatay.hazine.playground.message_to_admin"></activity>
        <activity android:name="something.about.hatay.hazine.admin.second_panel" />
        <activity android:name="something.about.hatay.hazine.admin.first_panel" />
        <activity android:name="something.about.hatay.hazine.playground.main_playground" />
        <activity android:name="something.about.hatay.hazine.admin.admin_choosing_paper" />
        <activity android:name="something.about.hatay.hazine.enter_things.new_username" />
        <activity android:name="something.about.hatay.hazine.enter_things.confirm_email" />
        <activity android:name="something.about.hatay.hazine.enter_things.forget_password" />
        <activity android:name="something.about.hatay.hazine.enter_things.registration" />
        <activity android:name="something.about.hatay.hazine.playground.comment" />
